{
  "name": "webpack-glsl-loader",
  "version": "1.0.1",
  "description": "A webpack loader for glsl shaders - includes support for nested imports.",
  "main": "dist/loader.js",
  "typings": "lib/loader.d.ts",
  "scripts": {
    "build": "npm run build:packages-cjs && npm run build:packages-es",
    "build:packages-cjs": "tsc --module commonjs --outDir dist --target es5",
    "build:packages-es": "tsc",
    "test": "npm run build && jest",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mentos1836/webpack-glsl-loader.git"
  },
  "keywords": [
    "glsl",
    "shader",
    "webgl",
    "webpack",
    "loader",
    "import",
    "include"
  ],
  "author": "Tine Jozelj <mentos1386@tjo.space> (https://blog.tjo.space)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mentos1386/webpack-glsl-loader/issues"
  },
  "homepage": "https://github.com/mentos1386/webpack-glsl-loader#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/*/*.spec.ts"
  },
  "devDependencies": {
    "@types/jest": "^21.1.6",
    "@types/loader-utils": "^1.1.0",
    "@types/node": "^8.0.51",
    "jest": "^21.2.1",
    "loader-utils": "^1.1.0",
    "memory-fs": "^0.4.1",
    "ts-jest": "^21.2.1",
    "tsc": "^1.20150623.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {}
}
